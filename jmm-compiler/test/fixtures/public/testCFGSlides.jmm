class Coiso {
    public int coisoETal(int sum, int i, int[] A, int N) {
        int t1;

        sum = 0;                    // 0
        i = 0;                      // 1
        while (i < N) {             // 2
            t1 = A[i];              // 3
            sum = sum + t1;         // 4
            i = i + 1;              // 5
        }
        // 6

        return sum;                 // 7
    }
}