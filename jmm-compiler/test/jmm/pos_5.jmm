

class Fac {
    public int ComputeFac(int num, int[] b) {
        int num_aux ;
        b[2] = 2;
        num_aux = b[2];
        if (num < 1)
            num_aux = 1;
        else num_aux = num * (this.ComputeFac(num-1, b));

        num_aux = b[2];
        return num_aux;
    }

