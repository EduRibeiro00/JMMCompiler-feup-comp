class Fac extends University {
    public int ComputeFac(int num, boolean shouldDivideBy2) {
        int num_aux;
        int[] array;
        array = new int[10];
        num = this.num(12);

        if (num < 1) {
            num_aux = 1;
        }
        else {
            num_aux = num * (this.ComputeFac(num - 1));
        }

        while (num_aux < 2) {
            num_aux = num_aux / 2;
            array[this.num(12)] = num_aux;
            num = num + 1;
        }

        shouldDivideBy2 = false;

        return num_aux;
    }

    public Nota get20InComp(int grade) {
        int grade_final;
        Nota nota;
        grade_final = grade;
        nota = new Nota();
        if (grade_final < 20) {
            grade_final = 20;
        }
        else {
            while(20 < grade_final) {
                grade_final = grade_final - 1;
            }
        }

        nota.setNota(grade_final);
        return nota;
    }
}